import java.util.Scanner;

public class exe01 {

	public static void main(String[] args) {
	
		/*
			1.Faça um programa que leia dois valores numéricos inteiros e apresente o resultado da diferença 
			do maior valor pelo menor valor.Se os valores forem iguais, o programa deve mostrar zero.
		*/
		int n1,n2;
		Scanner input = new Scanner(System.in);
		
			n1 = input.nextInt();
			n2 = input.nextInt();
		
		if(n1 > n2) {
			
			System.out.printf("diferença é \n %d",n1-n2);
		
		}else if(n2 > n1) {
			
			System.out.printf("difença é \n %d", n2-n1);
		}else if (n1 == n2) {
			
			System.out.printf("diferença é %d ", n1-n2);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
