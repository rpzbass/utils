package variaveis.condicional;

public class OperadorTernario {

	public static void main(String[] args) {
		
		
		
		int N1,N2,N3,N4,MEDIA_FINAL;
		
		N1 = 50;
		N2 = 90;
		N3 = 100;
		N4 = 60;
		
		MEDIA_FINAL = (N1+N2+N3+N4)/4;
		
		(MEDIA_FINAL>=70)? System.out.print("ALUNO APROVADO!!!!"); : System.out.print("ALUNO REPROVADO"); 
		
		
		
		
		

	}

}
