package entidade;

public class Program {

	public static void main(String[] args) {
		
		Pessoa pessoa1 = new Pessoa("Von Neumann",30,83);  /* Pessoa(); é
		 								o construtor vazio para instanciar 
		 								 uma instancia pessoa1 do tipo Pessoa */ 		
		Pessoa pessoa2 = new Pessoa("Dennis Ritchie",30,83);
		

		System.out.println(pessoa1.toString());
		System.out.println(pessoa2.toString());

	}

}
