package variaveis;

public class Program {

	public static void main(String[] args) {

		/*expressoes + operadores aritmeticos*/
		
		
		
		System.out.println(" 2 + 1 = " + (2 + 1));
		
		
		
		System.out.println(" 2 * 4 = " + 2 * 4);
		
		 
		
		System.out.println(" 4 / 2 = " + 4 / 2);
				
		
		System.out.println(" 10 - 4 = " + (10 - 4));
		
		
		
		System.out.println(" 4 % 2 = " + 4 % 2);
		
		
		
	}

}
